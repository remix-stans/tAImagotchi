{
  "name": "react-router-cloudflare-minimal",
  "private": true,
  "type": "module",
  "scripts": {
    "generate": "pnpm rr:typegen && pnpm cf:typegen && pnpm db:generate",
    "dev": "vite dev",
    "build": "vite build",
    "start": "vite preview",
    "deploy": "pnpm run build && wrangler deploy",
    "typecheck": "pnpm generate && tsc",
    "rr:typegen": "react-router typegen",
    "rr:routes": "react-router routes",
    "cf:typegen": "wrangler types",
    "cf:dev": "wrangler dev",
    "sly": "pnpx @sly-cli/sly add",
    "db:migrate": "wrangler d1 migrations apply db",
    "db:generate": "pnpm kysely-codegen --url $(find ./.wrangler/state/v3/d1/miniflare-D1DatabaseObject -name \"*.sqlite\" | head -n 1) --dialect sqlite",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "fix": "biome check --write --unsafe ."
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.5",
    "@ai-sdk/react": "^1.2.5",
    "@base-ui-components/react": "1.0.0-alpha.7",
    "@conform-to/react": "^1.3.0",
    "@conform-to/zod": "^1.3.0",
    "@openrouter/ai-sdk-provider": "^0.4.5",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-router/cloudflare": "^7.4.1",
    "@react-router/serve": "^7.4.1",
    "agents": "^0.0.49",
    "ai": "^4.2.10",
    "better-auth": "1.2.6-beta.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "hono": "^4.7.5",
    "hono-agents": "^0.0.39",
    "isbot": "^5.1.25",
    "kysely": "^0.27.6",
    "kysely-d1": "^0.3.0",
    "lucide-react": "^0.487.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.4.1",
    "tailwind-merge": "^3.1.0",
    "tw-animate-css": "^1.2.5",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/vite-plugin": "^0.1.20",
    "@cloudflare/workers-types": "^4.20250403.0",
    "@react-router/dev": "^7.4.1",
    "@sly-cli/sly": "^2.1.1",
    "@tailwindcss/vite": "^4.1.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "better-sqlite3": "^11.9.1",
    "kysely-codegen": "^0.18.3",
    "react-router-devtools": "^1.1.8",
    "tailwindcss": "^4.1.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.4",
    "vite-plugin-icons-spritesheet": "^3.0.1",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.7.0"
  },
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808",
  "pnpm": {
    "onlyBuiltDependencies": ["better-sqlite3"]
  }
