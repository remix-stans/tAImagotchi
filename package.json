{
  "name": "react-router-cloudflare-minimal",
  "private": true,
  "type": "module",
  "scripts": {
    "generate": "pnpm rr:typegen && pnpm cf:typegen && pnpm db:generate",
    "dev": "vite dev",
    "build": "vite build",
    "start": "vite preview",
    "deploy": "pnpm run build && wrangler deploy",
    "typecheck": "pnpm generate && tsc",
    "rr:typegen": "react-router typegen",
    "rr:routes": "react-router routes",
    "cf:typegen": "wrangler types",
    "cf:dev": "wrangler dev",
    "sly": "pnpx @sly-cli/sly add",
    "db:migrate": "wrangler d1 migrations apply db",
    "db:generate": "pnpm kysely-codegen --url $(find ./.wrangler/state/v3/d1/miniflare-D1DatabaseObject -name \"*.sqlite\" | head -n 1) --dialect sqlite",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "fix": "biome check --write --unsafe ."
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.3",
    "@ai-sdk/react": "^1.2.2",
    "@base-ui-components/react": "1.0.0-alpha.7",
    "@conform-to/react": "^1.3.0",
    "@conform-to/zod": "^1.3.0",
    "@openrouter/ai-sdk-provider": "^0.4.5",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-router/cloudflare": "^7.4.0",
    "@react-router/serve": "^7.4.0",
    "agents": "^0.0.46",
    "ai": "^4.2.6",
    "better-auth": "^1.2.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "hono": "^4.7.5",
    "hono-agents": "^0.0.36",
    "isbot": "^5.1.25",
    "kysely": "^0.27.6",
    "kysely-d1": "^0.3.0",
    "lucide-react": "^0.484.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.4.0",
    "tailwind-merge": "^3.0.2",
    "tw-animate-css": "^1.2.4",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/vite-plugin": "^0.1.18",
    "@cloudflare/workers-types": "^4.20250327.0",
    "@react-router/dev": "^7.4.0",
    "@sly-cli/sly": "^2.1.1",
    "@tailwindcss/vite": "^4.0.17",
    "@total-typescript/ts-reset": "^0.6.1",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "better-sqlite3": "^11.9.1",
    "kysely-codegen": "^0.18.3",
    "react-router-devtools": "^1.1.7",
    "tailwindcss": "^4.0.17",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-icons-spritesheet": "^3.0.1",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.5.1"
  },
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
